    // "@helper": [
    //   "src/helper"
    // ],
    // "@instances": [
    //   "src/instances"
    // ],
    // "@interfaces": [
    //   "src/interfaces"
    // ],
    // "@navigation": [
    //   "src/navigation"
    // ],
    // "@redux": [
    //   "src/redux"
    // ],
    // "@screens": [
    //   "src/screens"
    // ],
    // "@services": [
    //   "src/services"
    // ],
    "exclude": [
      "node_modules",
      "babel.config.js",
      "metro.config.js",
      "jest.config.js"
    ]
  }
}